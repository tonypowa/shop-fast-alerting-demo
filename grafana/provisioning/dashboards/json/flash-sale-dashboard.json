          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  p.name as \"Product\",\n  COUNT(o.id) as \"Orders\",\n  SUM(o.quantity) as \"Units Sold\",\n  CONCAT('$', ROUND(SUM(o.total_amount)::numeric, 2)) as \"Revenue\",\n  p.stock_level as \"Stock Left\"\nFROM orders o\nJOIN products p ON o.product_id = p.id\nWHERE o.order_time > NOW() - INTERVAL '1 hour'\nGROUP BY p.id, p.name, p.stock_level\nORDER BY SUM(o.total_amount) DESC\nLIMIT 10",
          "refId": "A",
          "sql": {
